	"exports": "./dist/index.js",
	"types": "./index.d.ts",
	"engines": {
		"node": ">=14.16"
	},
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/knutkirkhorn/cisco-vpn-rdp-connecter/issues"
	},
	"homepage": "https://github.com/knutkirkhorn/cisco-vpn-rdp-connecter#readme",
	"dependencies": {
		"cisco-vpn": "^1.0.2",
		"conf": "^11.0.1",
